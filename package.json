{
  "name": "ogitart",
  "version": "0.0.1",
  "description": "Commande de sandwichs",
  "main": "server.js",
  "repository": "https://github.com/jbruggem/ogitart.git",
  "scripts": {
    "test": "npm run lint && mocha",
    "build": "npm install --only=production && sh build.sh",
    "mon": "nodemon ogitart.js",
    "lint": "jshint src/",
    "start": "NODE_ENV=production forever -m5 start ./ogitart.js",
    "restart": "NODE_ENV=production forever restart ./ogitart.js",
    "stop": "forever stop ./ogitart.js",
    "logs": "forever logs"
  },
  "author": "Jehan Bruggeman <jbruggem@symzo.be>",
  "license": "Apache-2.0",
  "dependencies": {
    "body-parser": "^1.18.3",
    "bootstrap": "^4.1.2",
    "cors": "^2.8.4",
    "diacritics": "^1.2.1",
    "express": "^4.16.3",
    "jfs": "^0.3.0",
    "jquery": "^3.0.0",
    "lodash": "^4.17.10",
    "mkdirp": "^0.5.1",
    "typeahead.js": "^0.11.1",
    "browserify": "^16.2.2",
    "forever": "^0.15.3"
  },
  "devDependencies": {
    "jshint": "^2.9.6",
    "mocha": "^5.2.0",
    "nodemon": "^1.17.5",
    "rimraf": "^2.6.2",
    "should": "^13.2.1",
    "supertest": "^3.1.0"
  }
}
